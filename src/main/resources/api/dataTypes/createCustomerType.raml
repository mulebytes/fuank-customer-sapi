#%RAML 1.0 DataType
type: object
properties:
  firstName:
    type: string
    required: true
    example: "Ulrich"
  lastName:
    type: string
    required: true
    example: "Nielsen"
  companyName:
    type: string
    required: true
    example: "E-ffoc"
  email:
    type: string
    required: true
    example: "ulrichN@effoc.com"
  phone:
    type: string
    required: true
    example: "+494341927420"
  address:
    type: !include addressType.raml
    required: false 